    },
    "formOutput" : {
      "type" : "expression",
      "value" : [
        "if( $data.reclamo ){",
        "\treturn {",
        "\t\t//map reclamo variable to expected task contract input",
        "\t\treclamoInput: {",
        "\t\t\tid: $data.reclamo.id !== undefined ? $data.reclamo.id : null,",
        "\t\t\ttipoReclamo: $data.reclamo.tipoReclamo !== undefined ? $data.reclamo.tipoReclamo : null,",
        "\t\t\tdescripcion: $data.reclamo.descripcion !== undefined ? $data.reclamo.descripcion : null,",
        "\t\t\tfecha: $data.reclamo.fecha !== undefined ? $data.reclamo.fecha : null,",
        "\t\t\testadoAprobado: $data.reclamo.estadoAprobado !== undefined ? $data.reclamo.estadoAprobado : null",
        "\t\t}",
        "\t}",
        "}"
      ],
      "exposed" : false
    },
